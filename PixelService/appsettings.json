{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RabbitMQ": {
    "HostName": "rabbitmq",
    "ExchangeName": "dlx.exchange.visitLog",
    "VisitsLogQueueName": "visitsLogQueue",
    "DeadLetterQueueName": "visitLogDlq",
    "DeadLetterRoutingKey": "visitLogDlqKey",
    "MessageTTL": 86400000,
    "HeartbeatInSeconds": 60
  }
}
